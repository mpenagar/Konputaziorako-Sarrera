    "def f1_1txanda(fizena,kodif=\"utf8\"):\n",
    "    z = []\n",
    "    f = open(fizena,encoding=kodif)\n",
    "    for ilara in f :\n",
    "        x = ilara.split()\n",
    "        b = 0\n",
    "        for p in x[1:]:\n",
    "            b += int(p)\n",
    "        z.append((x[0],b))\n",
    "    f.close()\n",
    "    return z\n",
    "\n",
    "f1_1txanda(\"bizkaia_utf8.txt\")"
   ]
